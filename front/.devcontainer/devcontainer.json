{
	"name": "opencv",
	"dockerComposeFile": [
		"../../docker-compose.yml"
	],
	"service": "front",
	"workspaceFolder": "/front",
	"extensions": [
		"dbaeumer.vscode-eslint",
		"esbenp.prettier-vscode",
		"johnsoncodehk.volar",
	],
	"settings": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	  },
	"postAttachCommand": "npm install",
	"shutdownAction": "stopCompose"
}